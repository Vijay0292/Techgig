#include <iostream>
using namespace std;
int main()
{
  int x = 1;
  cin>>x;
  if (x<10)
  {
	  cout<<"I am happy as having no reponsibilites.";
  }
  else if (x>=10 && x <18)
  {
	  cout<<"I am still happy but starts feeling pressure of life.";
  }
  else{
	  cout<<"I am very much happy as i handled the pressure very well.";
  }
  return 0;
}
